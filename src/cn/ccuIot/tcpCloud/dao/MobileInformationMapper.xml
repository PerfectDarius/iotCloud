<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.ccuIot.tcpCloud.dao.MobileInformationMapper">

    <insert id="addMobileInformation" parameterType="mobileInformation">
        insert into mobileInformation
        (message, time)
        values
        (#{message},#{time})
    </insert>
</mapper>